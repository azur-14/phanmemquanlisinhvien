<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/> <!-- Tăng radius để tạo hình tròn -->
            <solid android:color="#ffffff"/> <!-- Màu nền trắng -->
            <stroke android:color="#808080" android:width="2dp"/> <!-- Đổi màu viền thành xám -->
        </shape>
    </item>
</selector>
